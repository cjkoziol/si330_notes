    "def get_state(cell): #creates function get_state and takes cell\n",
    "    import re #import regex\n",
    "    results = re.findall(r'([A-Z]{2}) (?:\\d{5})', cell) #find all the state codes and non capture zipcode in every cell\n",
    "    return results[0] #[0] removes the brackets around the state names \n",
    "\n",
    "person_info.applymap(get_state).head() #dataframe applies map to the function"
   ]
